XF86Audio{Prev,Next}
	mpc -q {prev,next}

XF86MonBrightness{Up,Down}
	xbacklight {+,-}10

XF86Audio{Raise,Lower}Volume
	amixer set Master 10{+,-}

XF86AudioMute
	amixer set Master toggle

XF86AudioPlay
	mpc toggle

Print
	flameshot gui -p ~/Pictures/Screenshots/

super + x
	/home/booterror/.local/bin/run-xscreenlock.sh
